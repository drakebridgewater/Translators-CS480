(
    ( stdout ( + 6.1 7 ) )
    ( stdout ( - 6 7 ) )
    ( stdout ( * 6.1 7 ) )

    ( stdout ( + 1.1 ( + 5 4 ) ) )
    ( stdout ( - 2.2 3 ) )
    ( stdout ( * 3 4.4 ) )
    ( stdout ( / 4 9 ) )

    ( stdout ( + 10 2 ) )
    ( stdout ( - 20 3 ) )
    ( stdout ( * 30 4 ) )
    ( stdout ( / 40 4 ) )

    ( let ( ( x int ) ) )
    ( := x 10.0 )
    ( stdout ( - 20 x ) )

    ( if ( > x 3 )
        ( stdout "yes" )
    )
    ( if ( < x 3 )
        ( stdout  ( + "yes" " true" ) )
        ( stdout ( + "no" " true" ) )
    )
    ( while ( > x 5 )
        ( stdout "hello" )
        ( := x ( - x 1 ) )
    )
)