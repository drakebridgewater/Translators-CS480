(
( + "abc" "def")
( + 6.1 7 )
( - 6 7 )
( * 6.1 7 )

( + 1.1 ( + 5 4 ) )
( - 2.2 3 )
( * 3 4.4 )
( / 4 9 )

( + 10 2 )
( - 20 3 )
( * 30 4 )
( / 40 4 )
)

(
    ( let ( ( x int ) ) )
    ( := x 10.0 )
    ( stdout ( - 20 x ) )

    ( if ( > x 3 )
        ( stdout "yes" )
    )
    ( if ( < x 3 )
        ( stdout  ( + "yes" " true" ) )
        ( stdout ( + "no" " true" ) )
    )
    ( while ( > x 5 )
        ( stdout "hello" )
        ( := x ( - x 1 ) )
    )
)