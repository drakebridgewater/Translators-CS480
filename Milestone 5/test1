
(
    ( let ( ( x int ) ) )
    ( := x 10 )
    ( stdout ( - 20 x ) )

    ( if ( > x 1 )
        ( stdout "yes1" )
    )
    ( if ( < x 2 )
        ( stdout  ( + "yes2" " true" ) )
        ( stdout ( + "no2" " true" ) )
    )
    ( if ( < x 3 )
        ( stdout  ( + "yes3" " true" ) )
        ( stdout ( + "no3" " true" ) )
    )
    ( if ( < x 4 )
        ( stdout "yes4" )
    )
    ( := x 15 )
    ( while ( > x 5 )
        ( stdout "hello5" )
        ( := x ( - x 1 ) )
    )
    ( if ( > x 6 )
        ( stdout "yes6" )
    )
)